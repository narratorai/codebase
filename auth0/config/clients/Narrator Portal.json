{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "Narrator Portal",
  "is_first_party": true,
  "oidc_conformant": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "description": "User Facing Portal UI",
  "logo_uri": "https://assets.narrator.ai/brand/narrator-logo-mark-150.png",
  "sso": true,
  "callbacks": [
    "https://##CUSTOM_TENANT_DOMAIN##/login/callback",
    "http://localhost:3000/callback",
    "http://localhost:3001/callback",
    "https://local.narrator.ai:3000/callback",
    "https://portal.narrator.ai/callback",
    "https://portal-nonprod.narrator.ai/callback",
    "https://portal-*.dev.narrator.ai/callback",
    "https://portal.eu.narrator.ai/callback"
  ],
  "allowed_logout_urls": [
    "http://localhost:3000",
    "http://localhost:3001",
    "https://local.narrator.ai:3000",
    "https://*.narrator.ai",
    "https://*.portal.narrator.ai",
    "https://portal-nonprod.narrator.ai",
    "https://portal-*.dev.narrator.ai",
    "https://portal.eu.narrator.ai/"
  ],
  "allowed_clients": [],
  "native_social_login": {
    "apple": {
      "enabled": false
    },
    "facebook": {
      "enabled": false
    }
  },
  "refresh_token": {
    "leeway": 120,
    "token_lifetime": 2592000,
    "expiration_type": "expiring",
    "idle_token_lifetime": 864000,
    "infinite_idle_token_lifetime": false,
    "infinite_token_lifetime": false,
    "rotation_type": "rotating"
  },
  "initiate_login_uri": "https://portal.narrator.ai",
  "allowed_origins": [],
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 259200,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "none",
  "app_type": "spa",
  "grant_types": ["authorization_code", "implicit", "refresh_token"],
  "web_origins": [
    "http://localhost:3000",
    "http://localhost:3001",
    "https://local.narrator.ai:3000",
    "https://portal.narrator.ai",
    "https://portal-nonprod.narrator.ai",
    "https://portal-*.dev.narrator.ai"
  ],
  "custom_login_page_on": true
}
