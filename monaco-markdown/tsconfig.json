{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["dom", "es5", "es7"]
  }
}
