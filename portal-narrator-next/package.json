{
  "name": "portal-next",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "stylelint --fix --cache --allow-empty-input"
    ],
    "*.graphql": [
      "yarn narrator-graph-codegen --root portal",
      "yarn lint"
    ]
  },
  "scripts": {
    "dev": "concurrently --kill-others --raw 'narrator-graph-codegen --watch --root portal' 'next dev'",
    "build": "NODE_OPTIONS=--max-old-space-size=7936 next build",
    "start": "NODE_OPTIONS=--max-old-space-size=7936 next start",
    "postinstall": "./scripts/postinstall.sh",
    "lint": "eslint --cache",
    "lint:all": "yarn lint './{src,portal}/**/*.{ts,js,tsx,jsx}'",
    "test": "NODE_OPTIONS='--enable-source-maps --unhandled-rejections=strict --trace-warnings' jest",
    "test:e2e": "cypress open --e2e --browser chrome",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
